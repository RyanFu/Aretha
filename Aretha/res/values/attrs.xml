<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="duration" format="integer" />
    <attr name="dotRadius" format="dimension" />
    <attr name="dotSpacing" format="dimension" />
    <attr name="dotColor" format="color" />
    <attr name="radius" format="dimension" />

    <declare-styleable name="SectorView">
        <attr name="radius" />
        <attr name="quadrant">
            <enum name="center" value="0" />
            <enum name="leftBottom" value="1" />
            <enum name="rightBottom" value="2" />
            <enum name="rightTop" value="3" />
            <enum name="leftTop" value="4" />
            <enum name="top" value="5" />
            <enum name="left" value="6" />
            <enum name="bottom" value="7" />
            <enum name="right" value="8" />
        </attr>
        <attr name="animationOffset" format="integer" />
        <attr name="duration" />
    </declare-styleable>
    <declare-styleable name="AccelerateLinearDecelerateProgressBar">
        <attr name="dotCount" format="integer" />
        <attr name="duration" />
        <attr name="dotRadius" />
        <attr name="dotSpacing" />
        <attr name="dotColor" />
    </declare-styleable>
    <declare-styleable name="PageIndicator">
        <attr name="activePage" format="integer" />
        <attr name="pageCount" format="integer" />
        <attr name="dotRadius" />
        <attr name="dotSpacing" />
        <attr name="dotColor" />
    </declare-styleable>
    <declare-styleable name="ToggleView">
        <attr name="toggle">
            <enum name="on" value="1" />
            <enum name="off" value="0" />
        </attr>
        <attr name="radius" />
    </declare-styleable>
    <declare-styleable name="Workspace">
        <attr name="showChild" format="integer" />
        <!-- define the min velocity to perform scroll page action -->
        <attr name="snapVelocity" format="integer">
			<enum name="auto" value="-1" />            
        </attr>
        <attr name="duration" />
    </declare-styleable>
    <declare-styleable name="ClickWheelView">
        <attr name="gapDegree" format="float" />
    </declare-styleable>

</resources>